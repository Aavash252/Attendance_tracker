@model IEnumerable<FinalProject.Areas.Identity.Data.FinalProjectUser>

@{
    ViewData["Title"] = "List";
}

<body style="background-color:powderblue;">
<table>
    <tr>
        <th>User Name</th>
        <th>Email</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>
                <a href="@Url.Action("Info", "User", new { userId = user.Id })">@user.UserName</a>
            </td>
            <td>@user.Email</td>
        </tr>
    }
</table>
</body>





