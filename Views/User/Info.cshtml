@model FinalProject.Areas.Identity.Data.FinalProjectUser

@{
    ViewData["Title"] = "Info";
}

<h1>User Details</h1>

<table class="table">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Email</th>
            <th>Clock In</th>
            <th>Clock Out</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.UserName</td>
            <td>@Model.Email</td>
          <td>@(ViewBag.TimeRecords.ClockIn)</td>
            <td>@(ViewBag.TimeRecords.ClockOut) </td>
     
        </tr>
    </tbody>
</table>
